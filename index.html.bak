<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <TITLE>IronLogic Bootloader</TITLE>
     <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="style.css">
</head>
<body>
<div class='main'>
    <div class='top'>IronLogic Bootloader</div>
    <div class='content'>
		<div class='centre'>
			<div data-line='login'>
				<div></div>
				<input  type="text">
			</div>
			<div data-line='password'>
				<div></div>
				<input  type="text">
			</div>
			<div data-line='good'>
				<div></div>
				<input  type="text" value="M-VI K Net NFC">
			</div>
			<div data-line='number'>
				<div></div>
				<input  type="text" value="All">
			</div>
			<div data-line='version'>
				<div></div>
				<input  type="text">
			</div>
			<div data-line='description'>
				<div></div>
				<input  type="text">
			</div>
			<div data-line='firmware'>
				<div></div>
				<input  type="file">
			</div>
        </div>
        <div class='section'>
			<div data-click='out' title = "Выйти"></div>		
			<div data-click='save' title = "Сохранить"></div>
			<div data-click='send' title = "Отправить"></div>
        </div>
    </div>
</div>
<script src="script.js"></script>
</body>
</html>